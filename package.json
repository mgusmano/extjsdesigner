{
	"name": "extjsdesigner",
	"galleryBanner": {
		"color": "#04364b",
		"theme": "dark"
	},
	"icon": "senchaleaf.png",
	"displayName": "extjsdesigner",
	"description": "Sencha Ext JS Designer",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.46.0"
	},
	"keywords": [
		"Sencha",
		"Ext JS",
		"Architect"
	],
	"publisher": "marc-gusmano",
	"author": "Marc Gusmano",
	"license": "MIT",
	"homepage": "https://github.com/mgusmano/extjsdesigner",
	"repository": {
		"type": "git",
		"url": "https://github.com/mgusmano/extjsdesigner"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onCustomEditor:extjsdesigner.editor",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsWelcome": [
			{
				"view": "toolbarView",
				"contents": "Ext JS Designer Welcome"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "toolbarView",
					"name": "Ext JS Designer Toolbar"
				}
			]
		},
		"customEditors": [
			{
				"viewType": "extjsdesigner.instructions",
				"displayName": "Ext JS Designer Instructions",
				"selector": [
					{
						"filenamePattern": "*.mjg"
					}
				],
				"priority": "default"
			},
			{
				"viewType": "extjsdesigner.editor",
				"displayName": "Ext JS Designer Editor",
				"selector": [
					{
						"filenamePattern": "*View.js"
					},
					{
						"filenamePattern": "**/app/view/Main/List.js"
					}
				],
				"priority": "default"
			}
		],
		"commands": [
			{
				"command": "extjsdesigner.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"escodegen": "~1.14.3",
		"esprima": "~4.0.1",
		"fs-exists-sync": "~0.1.0",
		"monaco-editor": " ^0.20.0",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
