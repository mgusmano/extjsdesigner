{
	"name": "extjsdesigner",
	"displayName": "extjsdesigner",
	"description": "Sencha Ext JS Designer",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onCustomEditor:extjsdesigner.editor",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "extjsdesigner.editor",
				"displayName": "Ext JS Designer Editor",
				"selector": [
					{
						"filenamePattern": "*View.js"
					},
					{
						"filenamePattern": "**/app/view/Main/List.js"
					}
				],
				"priority": "default"
			}
		],
		"commands": [
			{
				"command": "extjsdesigner.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"fs-exists-sync": "~0.1.0"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
